#include <stdio.h>
#include "ft_printf.h"
#include <limits.h>

#define txt "%1.*s",0,NULL

int main()
{
	int retur = 0;
	int r = 0;
	retur = ft_printf(txt);
	printf("{%d}\n",retur);
	printf("\n");
	retur = printf(txt);
	printf("{%d}\n",retur);
	return 0;
}
